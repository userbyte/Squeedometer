  "authors": [
    "squeek", "mkpoli", "Queueue_"
  ],
  "contact": {
    "homepage": "https://github.com/Queueue0/Squeedometer/releases/",
    "sources": "https://github.com/Queueue0/Squeedometer"
  },

  "license": "CC0-1.0",
  "icon": "assets/squeedometer/icon.png",

  "environment": "client",
